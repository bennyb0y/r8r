{
  "name": "r8r",
  "version": "0.1.0",
  "description": "Multi-tenant SaaS platform for custom rating sites - subdomain-based communities (pizza.r8r.one, coffee.r8r.one, etc.)",
  "private": true,
  "homepage": "https://r8r.one",
  "repository": {
    "type": "git",
    "url": "https://github.com/bennyb0y/r8r.git"
  },
  "keywords": [
    "rating",
    "reviews",
    "multi-tenant",
    "saas",
    "platform",
    "cloudflare",
    "nextjs"
  ],
  "author": "Benjamin Fischer",
  "scripts": {
    "dev": "next dev",
    "lint": "eslint . --ignore-pattern '.next/**' --ignore-pattern 'dist/**' --ignore-pattern 'out/**' --ignore-pattern 'deploy/**' --ignore-pattern '.vercel/**' --ignore-pattern '.wrangler/**'",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "deploy": "npm run deploy:worker && npm run build && npm run deploy:pages",
    "deploy:pages": "npx wrangler pages deploy out --project-name r8r-platform",
    "deploy:worker": "wrangler deploy --config wrangler.worker.toml",
    "deploy:full": "npm run deploy:worker && npm run build && npm run deploy:pages"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@react-google-maps/api": "^2.19.3",
    "@tremor/react": "^3.14.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ws": "^8.5.10",
    "cookie": "^1.0.2",
    "critters": "^0.0.20",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.12.0",
    "next": "^15.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-jsx": "^5.1.2",
    "typescript": "^5.4.2",
    "undici": "^6.9.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@cloudflare/workers-types": "^4.20250311.0",
    "@next/bundle-analyzer": "^15.2.2",
    "@types/google.maps": "^3.55.4",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "wrangler": "^4.19.1"
  },
  "overrides": {
    "path-to-regexp": "^6.2.3",
    "cookie": "^1.0.2"
  }
}
